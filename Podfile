# platform :ios, '9.0'

target 'HackIllinois: QR' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    pod 'SwiftQRCode'
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'JWTDecode'

    # Pods for HackIllinois: QR
    target 'HackIllinois: QRTests' do
        inherit! :search_paths
        # Pods for testing
    end

    target 'HackIllinois: QRUITests' do
        inherit! :search_paths
        # Pods for testing
    end
end
